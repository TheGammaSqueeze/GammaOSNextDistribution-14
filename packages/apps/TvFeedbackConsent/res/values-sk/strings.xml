<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="feedback_system_logs_title_text" msgid="8162964763101361628">"Zahŕňať denníky zariadenia (systém a aplikácie)"</string>
    <string name="feedback_system_logs_legal_text" msgid="4770068793128503597">"Zdieľaním všetkých denníkov systému a aplikácií v zariadení môžete odosielať Googlu podrobnosti, ako sú používateľské mená, poloha, identifikátory zariadení a informácie o sieti. Google pomocou nich odstraňuje technické problémy a zlepšuje služby. Viac sa dozviete na g.co/android/devicelogs."</string>
    <string name="feedback_bugreport_title_text" msgid="8863884817986711122">"Zahrnúť hlásenie chyby"</string>
    <string name="feedback_bugreport_legal_display_text" msgid="4486929548753044585">"Hlásenie chyby (z %1$s) zahŕňa údaje, ako sú používateľské mená, poloha, identifikátory zariadení a informácie o sieti, aby Googlu pomohlo porozumieť problému a odstrániť ho."</string>
    <string name="next" msgid="3308587969183798652">"Ďalej"</string>
    <string name="feedback_view_system_logs_button_text" msgid="1889709679017124924">"Zobraziť systémové denníky"</string>
    <string name="feedback_view_logs_dialog_title" msgid="1937861449450135462">"Systémové denníky"</string>
    <string name="feedback_view_logs_dialog_body" msgid="2496560711853644250">"Tieto denníky obsahujú informácie potrebné na to, aby sme na vašu spätnú väzbu mohli reagovať."</string>
    <string name="annotated_back_indicator_string" msgid="3197935392074998004">"Prejdite späť stlačením tlačidla SPÄŤ."</string>
    <string name="loading" msgid="1709842456101615353">"Načítava sa"</string>
</resources>
